/* components/w-tab-control/w-tab-control.wxss */
.tab-control{
  display: flex;
  height: 88rpx;
  line-height: 88rpx;
  background: #fff;
}

.tab-item{
  flex:1;
  text-align: center;
  font-size: 32rpx;
 
}

.active{
  color:#ff5777;
}
.active text{
  padding: 20rpx 10rpx;
  border-bottom: 6rpx solid #ff5777;
  
}
